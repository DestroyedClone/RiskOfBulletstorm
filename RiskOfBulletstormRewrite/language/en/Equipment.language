		"RISKOFBULLETSTORM_EQUIPMENT__PICKUP": "",
		"RISKOFBULLETSTORM_EQUIPMENT__DESCRIPTION": "",
		"RISKOFBULLETSTORM_EQUIPMENT__LORE": "",
